      "visual" : {
        "at" : { "x" : 352, "y" : 160 }
      }
    }, {
      "id" : "f11",
      "type" : "Script",
      "name" : "find users",
      "config" : {
        "output" : {
          "code" : [
            "import ch.ivyteam.ivy.security.IUser;",
            "",
            "List<IUser> result;",
            "        ",
            "for(IUser user : ivy.security.users().paged()) {",
            "  if(user.getName().toUpperCase().contains(in.searchText.toUpperCase()))",
            "  {",
            "    result.add(user);",
            "  }",
            "}",
            "",
            "in.searchResult = result;"
          ]
        }
      },
      "visual" : {
        "at" : { "x" : 224, "y" : 160 },
        "labelOffset" : { "x" : -27, "y" : -8 }
      },
      "connect" : { "id" : "f10", "to" : "f9" }
    }, {
      "id" : "f18",
