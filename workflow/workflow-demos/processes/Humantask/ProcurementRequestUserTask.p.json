        "startCategory" : "human",
        "case" : {
          "category" : "Procurement/Request",
          "attachToBusinessCase" : false
        },
        "startDescription" : "<%=ivy.cms.co(\"/ProcessDescriptions/procurementRequestUserTaskDescription\")%>",
        "startCustomFields" : [
          { "name" : "cssIcon", "value" : "fa fa-female" }
        ],
        "task" : {
          "category" : "Input/Procurement"
        },
        "outLink" : "start.ivp",
        "startName" : "2.1: <%=ivy.cms.co(\"/ProcessDescriptions/procurementRequest\")%> (<%=ivy.cms.co(\"/ProcessDescriptions/userTaskExample\")%>)"
      },
      "tags" : [
        "demo"
      ],
      "visual" : {
        "at" : { "x" : 96, "y" : 192 },
