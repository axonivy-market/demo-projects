{
    "properties": {
		"importTestProject": false
	},
    "installers": [
        {
            "id": "maven-import",
            "data": {
                "projects": [
                    {
                        "groupId": "ch.ivyteam.demo",
                        "artifactId": "workflow-demos",
                        "version": "${version}",
                        "type": "iar"
                    },
                    {
                        "groupId": "ch.ivyteam.demo",
                        "artifactId": "workflow-demos-test",
                        "version": "${version}",
                        "type": "iar",
                        "importInWorkspace": "@importTestProject@"
                    }
                ],
                "repositories": [
                    {
                        "id": "maven.axonivy.com",
                        "url": "https://maven.axonivy.com",
                        "snapshots": {
                            "enabled": "true"
                        }
                    }
                ]
            }
        }
    ]
}
