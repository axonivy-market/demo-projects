Ss0 f10 actionCode 'import ch.ivyteam.ivy.workflow.ui.utils.UserSubstituteHelper;
import ch.ivyteam.ivy.security.IUserSubstitute;
import ch.ivyteam.ivy.security.IRole;
import ch.ivyteam.ivy.security.IUser;


//reset error message
out.errorMessage = "";

ivy.log.debug("START setting substitute of user {0}.", in.user.getName());


if (in.user is initialized)
{
	try
	{
		// substitute for personal tasks
		if (panel.personalRadioButton.isSelected()){
			
			ivy.log.debug("start substitute for personal tasks of user {0}.", in.user.getName());
	
				if (panel.substituteLookupTextField.getSelectedListEntry() instanceof IUser){
						UserSubstituteHelper.createSubstitute(in.user, panel.substituteLookupTextField.selectedListEntry as IUser, null, out.remark);
						out.remark = "";	
				}
				
		// substitute for the role
		} else if(panel.roleRadioButton.isSelected()){
			
			ivy.log.debug("start substitute for roles of user {0}.", in.user.getName());
			
			if (panel.substituteLookupTextField.getSelectedListEntry() instanceof IUser && panel.roleLookupTextField.getSelectedListEntry() instanceof IRole)
				{
						UserSubstituteHelper.createSubstitute(in.user, panel.substituteLookupTextField.selectedListEntry as IUser,panel.roleLookupTextField.selectedListEntry as IRole,out.remark);
						out.remark = "";					
				} else {
					out.errorMessage = ivy.cms.co("/ch/ivyteam/ivy/workflow/ui/common/plainStrings/invalidEntry");
		}
					
		// substitute for all my tasks
		} else if (panel.personalAllRolesRadioButton.isSelected()){
			
			ivy.log.debug("start substitute for all user tasks of user {0}.", in.user.getName() );
	
			//Delete all existing substitutions first
			//Then add all roles and personal
			for(IUserSubstitute substitute : in.substituteList){
				UserSubstituteHelper.deleteSubstitute(in.user, substitute);
			}
			
			ivy.log.debug("All substitutions are removed now of user {0}.", in.user.getName());
	
			//add all roles
			for(IRole role : in.roleList){
				UserSubstituteHelper.createSubstitute(in.user, panel.substituteLookupTextField.selectedListEntry as IUser, role, out.remark);
			}
			
			ivy.log.debug("Substitutions are done for roles of user {0}.", in.user.getName());
					
			//add personal
			UserSubstituteHelper.createSubstitute(in.user, panel.substituteLookupTextField.selectedListEntry as IUser, null, out.remark);
					
		}
		ivy.log.debug("END of setting substitute of user {0}.", in.user.getName());
	
	}
	catch (Exception ex)
	{
		out.errorMessage = ex.getMessage();	
	}		
}' #txt
