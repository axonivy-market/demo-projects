Ts0 f27 actionCode 'import ch.ivyteam.ivy.addons.restricted.eventlog.EventLogHelper;

// set titles
// panel.taskInformationTabbedPane.setTitleAt(0, ivy.cms.co("/ch/ivyteam/ivy/workflow/ui/task/plainStrings/taskExecution"));

// documents
// if there is some documents then add bullet as a suffix
panel.taskInformationTabbedPane.setTitleAt(0, ivy.cms.co("/ch/ivyteam/ivy/workflow/ui/case/plainStrings/documents") + 
	(panel.fileManagerRDC.getDocumentsListInPath(panel.fileManagerRDC.getServerPath()).isEmpty()? "": " \u2022"));


// annotations
// set the bullet as a suffix if there is no notes
panel.taskInformationTabbedPane.setTitleAt(1, ivy.cms.co("/ch/ivyteam/ivy/workflow/ui/common/note/plainStrings/annotations") + 
	(in.task.getCase().getNotes().isEmpty() && in.task.getNotes().isEmpty()? "": " \u2022"));


// events
panel.taskInformationTabbedPane.setTitleAt(2, ivy.cms.co("/ch/ivyteam/ivy/addons/eventlog/plainStrings/eventLogs") + 
	(EventLogHelper.findBusinessEventLogsAsSystemUser(in.task.getCase()).isEmpty()? "": + " \u2022"));


// details
panel.taskInformationTabbedPane.setTitleAt(3, ivy.cms.co("/ch/ivyteam/ivy/workflow/ui/common/plainStrings/details") + " \u2022");' #txt
