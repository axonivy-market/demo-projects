proc "startReadProcess" [val processName]
{
	clearRuntimeLog
	let[val proc [format "soap\\/client\\/personClient\\/%s" $processName]]
	{
		startBpmProcess -projectName $project -ivpPath $proc
	}
}

let [val project "ConnectivityDemos"]
{
	// setup
	importArchive -directory "applications/samples" -projectName $project
	showOnlyUserRuntimeLog
	startEngines

	startReadProcess "read.ivp"
	assertRuntimeLogMsg "Person[firstname=Bruno,"

	startReadProcess "add.ivp"
	assertRuntimeLogMsg "person added"

	startReadProcess "delete.ivp"
	assertRuntimeLogMsg "deleted Person[firstname=Joshua"

	//finally
	closeProject -projectName $project
	removeProject -projectName $project -removeFromDisk true
}
