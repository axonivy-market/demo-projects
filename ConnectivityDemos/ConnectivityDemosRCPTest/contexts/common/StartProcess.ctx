proc "startBpmProcess" [val projectName] [val ivpPath]
{
	get-button Run | click -arrow
	get-menu "Launch Configurations..." | click
	with [get-window "Run Configurations"] {
	    get-tree | select "Process Start"
	    get-button "New launch configuration" | click
	    get-group "Process Start" | get-button "Browse..." | click
	    let [val item [format "%s - %s" $projectName $ivpPath]]
	    {
	    	get-window "Select a process start" | get-table 
	        	| select $item | double-click
	    }
	    get-button Run | click
	}
}

