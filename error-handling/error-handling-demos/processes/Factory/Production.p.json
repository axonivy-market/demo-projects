  "id" : "1500417E1DADBB7C",
  "kind" : "CALLABLE_SUB",
  "config" : {
    "data" : "error.handling.demo.FactoryData"
  },
  "elements" : [ {
      "id" : "f0",
      "type" : "CallSubStart",
      "name" : [
        "produceShoes",
        "(String,Number)"
      ],
      "config" : {
        "signature" : "produceShoes",
        "input" : {
          "params" : [
            { "name" : "model", "type" : "String", "desc" : "" },
            { "name" : "amount", "type" : "Number", "desc" : "" }
          ],
          "map" : {
            "out.amount" : "param.amount",
            "out.model" : "param.model"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 96, "y" : 224 },
