{
  "$schema" : "https://json-schema.axonivy.com/process/11.3.0/process.json",
  "id" : "150047A1589EB0D8",
  "config" : {
    "data" : "booking.BookingData"
  },
  "elements" : [ {
      "id" : "f0",
      "type" : "RequestStart",
      "name" : "start.ivp",
      "config" : {
        "signature" : "start",
        "input" : {
          "map" : {
            "out.customer.firstname" : "\"John\"",
            "out.customer.lastname" : "\"Smith\""
          }
        },
        "request" : {
          "name" : "Business Error interrupts the happy path and calls rollback activities"
        }
      },
      "visual" : {
        "at" : { "x" : 48, "y" : 128 },
        "labelOffset" : { "x" : 12, "y" : 37 }
      },
      "connect" : [
        { "id" : "f4", "to" : "f3" }
      ]
    }, {
      "id" : "f3",
      "type" : "DialogCall",
      "name" : [
        "select",
        "flight & car"
      ],
      "config" : {
        "dialog" : "error.handling.demo.SelectOptions:start(booking.Customer)",
        "call" : {
          "map" : {
            "param.customer" : "in.customer"
          }
        },
        "output" : {
          "map" : {
            "out" : "in",
            "out.carSize" : "result.carSize",
            "out.flight" : "result.flight"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 168, "y" : 128 }
      },
      "connect" : [
        { "id" : "f6", "to" : "f5" }
      ]
    }, {
      "id" : "f5",
      "type" : "SubProcessCall",
      "name" : "book flight",
      "config" : {
        "processCall" : "Booking/Service/Flight:bookFlight(booking.Flight,booking.Customer)"
      },
      "visual" : {
        "at" : { "x" : 320, "y" : 128 }
      },
      "boundaries" : [ {
          "id" : "Et0",
          "type" : "ErrorBoundaryEvent",
          "name" : "failed",
          "config" : {
            "errorCode" : "booking:flight:failed"
          },
          "visual" : {
            "at" : { "x" : 352, "y" : 160 }
          },
          "connect" : [
            { "id" : "f10", "to" : "f9", "via" : [ { "x" : 352, "y" : 288 } ] }
          ]
        } ],
      "connect" : [
        { "id" : "f8", "to" : "f7" }
      ]
    }, {
      "id" : "f7",
      "type" : "SubProcessCall",
      "name" : "book car",
      "config" : {
        "processCall" : "Booking/Service/Car:bookCar(String)"
      },
      "visual" : {
        "at" : { "x" : 480, "y" : 128 }
      },
      "boundaries" : [ {
          "id" : "Et1",
          "type" : "ErrorBoundaryEvent",
          "name" : "failed",
          "config" : {
            "errorCode" : "booking:car:failed"
          },
          "visual" : {
            "at" : { "x" : 512, "y" : 160 }
          },
          "connect" : [
            { "id" : "f12", "to" : "f11" }
          ]
        } ],
      "connect" : [
        { "id" : "f2", "to" : "f1" }
      ]
    }, {
      "id" : "f9",
      "type" : "TaskEndPage",
      "name" : "booking failed",
      "config" : {
        "page" : "view/booking-failed.xhtml"
      },
      "visual" : {
        "at" : { "x" : 752, "y" : 288 },
        "labelOffset" : { "x" : 14, "y" : 37 }
      }
    }, {
      "id" : "f11",
      "type" : "SubProcessCall",
      "name" : [
        "cancel flight",
        "booking"
      ],
      "config" : {
        "processCall" : "Booking/Service/Flight:cancelFlight(booking.Flight,booking.Customer)"
      },
      "visual" : {
        "at" : { "x" : 512, "y" : 232 },
        "description" : "compensate already booked flight"
      },
      "connect" : [
        { "id" : "f13", "to" : "f9", "via" : [ { "x" : 752, "y" : 232 } ] }
      ]
    }, {
      "id" : "f1",
      "type" : "EMail",
      "name" : [
        "confirmation",
        "mail"
      ],
      "config" : {
        "headers" : {
          "subject" : "Booking confirmation",
          "to" : "<%=in.customer.email%>"
        },
        "message" : {
          "body" : "..."
        }
      },
      "visual" : {
        "at" : { "x" : 632, "y" : 128 }
      },
      "connect" : [
        { "id" : "f15", "to" : "f14" }
      ]
    }, {
      "id" : "f16",
      "type" : "ProcessAnnotation",
      "name" : [
        "Demonstrates the handling of Business Errors",
        "The Happy Path is interrupted and ",
        " rollback activities are called. "
      ],
      "visual" : {
        "at" : { "x" : 232, "y" : 48 },
        "size" : { "width" : 336 },
        "color" : "NodeStyle3"
      }
    }, {
      "id" : "f14",
      "type" : "TaskEndPage",
      "name" : [
        "booking ",
        "successful"
      ],
      "config" : {
        "page" : "view/booking-success.xhtml"
      },
      "visual" : {
        "at" : { "x" : 752, "y" : 128 },
        "labelOffset" : { "x" : 17, "y" : 41 }
      }
    } ],
  "layout" : {
    "colors" : {
      "NodeStyle3" : "rgb(255, 0, 0)"
    }
  }
}