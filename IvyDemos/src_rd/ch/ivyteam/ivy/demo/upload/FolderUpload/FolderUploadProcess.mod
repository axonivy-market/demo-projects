Fs0 f6 actionCode 'import ch.ivyteam.ivy.demo.upload.FileUploadManager;
panel.stopPollingTimer();

if(!in.zipUtil.isCancel())
{
	in.currentState = "Uploading";

	if(in.currentState.equals("Uploading"))
	{
		panel.statusTextArea.appendLine("Folder is compressed.");
		panel.statusTextArea.appendLine("Currently, the file is stored at " + in.zipUtil.getZipTempPath() + ".");
		panel.statusTextArea.appendLine("=====================================");
		in.uploadUtil = new FileUploadManager(in.zipUtil.getZipTempPath());
		in.uploadUtil.upload();
		panel.statusLabel.setText("Uploading...");
		panel.statusTextArea.appendLine("Prepair to upload file to server.");
		panel.statusTextArea.appendLine("File is uploading to server.");
		panel.startPollingTimer("Uploading");
	}
}
else
{
	panel.statusTextArea.setText("");
	panel.statusLabel.setText("Compressing cancelled.");
}





