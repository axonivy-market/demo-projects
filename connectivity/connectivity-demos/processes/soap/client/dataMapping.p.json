        "description" : [
          "sends and consumes complex objects:",
          "- for demo reasons, the remote service is also provided with axon.ivy tooling."
        ]
      },
      "connect" : [
        { "id" : "f9", "to" : "f8" }
      ]
    }, {
      "id" : "f8",
      "type" : "WebServiceCall",
      "name" : [
        "addPerson",
        "to own service"
