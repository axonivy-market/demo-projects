        "signature" : "mapComplexData",
        "request" : {
          "name" : "9.1.2 map complex data",
          "category" : "soap",
          "customFields" : [
            { "name" : "cssIcon", "value" : "fa fa-object-group" }
          ]
        }
      },
      "visual" : {
        "at" : { "x" : 96, "y" : 152 },
